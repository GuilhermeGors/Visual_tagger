{
  "version": 2,
  "builds": [
    { "src": "backend/src/main.py", "use": "@vercel/python" },
    { "src": "frontend/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "backend/src/main.py" },
    { "src": "/(.*)", "dest": "frontend/public/index.html" }
  ]
}
